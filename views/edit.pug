extends layout

block content
    h1 Edit a chat room

    form(method="post")
        fieldset.form-group
            label(for="name") Name :
            input.form-control(name ="name", type="text", placeholder="Entrer un nom de la Room", value = room.name)

        button.btn.btn-primary(type="submit") Save chat room name
    a.btn.btn-primary(href="/admin/rooms") Back

